# Exa MCP Server Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Your Exa API key from https://dashboard.exa.ai/api-keys
EXA_API_KEY=your_exa_api_key_here

# Optional: Node environment (development, production, test)
NODE_ENV=development

# Optional: Debug logging (set to true for verbose output)
DEBUG=false